{
  "name": "lwlightning",
  "version": "1.0.0",
  "repository": "git@github.com:lightwebs/lightning.git",
  "author": "Johan Carleborn <johancarleborn@gmail.com>",
  "scripts": {
    "clean": "rm -r dist/* .parcel-cache/*",
    "serve": "parcel --port 1234 --hmr-port 1235 src/js/app.js",
    "start": "parcel watch src/js/app.js",
    "build": "parcel build src/js/app.js",
    "colors": "node theme-scripts/colors.js"
  },
  "watchreload": {
    "files": "**/*.php"
  },
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.36",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "buffer": "^6.0.3",
    "cssnano": "5.1.13",
    "parcel": "^2.8.3",
    "parcel-plugin-watch-reload": "^1.0.3",
    "postcss": "^8.4.18",
    "postcss-cli": "10.0.0",
    "postcss-import": "15.0.0",
    "postcss-nested": "^6.0.1",
    "process": "^0.11.10",
    "tailwindcss": "3.2.7",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.14",
    "swiper": "9.0.5",
    "yarn": "^1.22.19"
  }
}
